{
  "name": "fballiano/mcommerce",
  "description": "A fork of OpenMage v20",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "type": "magento-source",
  "require": {
    "php": ">=7.4 <8.1",
    "magento-hackathon/magento-composer-installer": "^3.1 || ^2.1 || ^4.0",
    "shardj/zf1-future": "^1.21.2",
    "pelago/emogrifier": "^6.0",
    "pear/net_idna2": "^0.2.0",
    "fileeye/linuxlibertine-fonts": "^5.3.0",
    "phpseclib/phpseclib": "^3.0.14",
    "phpseclib/mcrypt_compat": "^2.0.3"
  },
  "suggest": {
    "colinmollenhour/cache-backend-redis": "^1.14.3",
    "colinmollenhour/magento-redis-session": "^2.4.0",
    "fballiano/magento1-image-cleaner": "^0.9.0",
    "gordonlesti/lesti_fpc": "^1.4.9"
  },
  "require-dev": {
    "macopedia/phpstan-magento1": "^1.0.4",
    "phpstan/phpstan": "^1.7.1"
  },
  "autoload-dev": {
    "psr-4": { "OpenMage\\Tests\\Unit\\": "dev/tests/unit" }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "mcommerce"
    },
    "magento-root-dir": "."
  },
  "config": {
    "allow-plugins": {
      "magento-hackathon/magento-composer-installer": true
    }
  }
}
